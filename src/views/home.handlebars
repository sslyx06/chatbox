<head>
  <link
    href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
    rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
    crossorigin="anonymous"
  />
</head>
</div>
<div>
  <div>
    <div class="ms-5 addProductsBox">
      <h3 class="mt-4 ms-3"> <u>Agrega Nuevos productos:</u>  </h3>
      <form class="d-flex flex-column" id="productForm1">
        <div class="d-flex flex-column ms-4">
          <label for="title">Title</label>
          <input id="title" name="title" />
          <label for="price">Price</label>
          <input id="price" name="price" />
          <label>Thumbnail</label>
          <input name="thumbnail" required />
          <input class="w-25 m-auto mt-3 " type="submit" />
        </div>
      </form>
    </div>
    <div class="ms-5 me-5">
      <h3 class="ms-3">Product View:</h3>
      <table class="table table-bordered table-striped text-center mt-4">
        <thead>
          <tr>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Thumbnail</th>
          </tr>
        </thead>

        <tbody id="productList"></tbody>
      </table>
    </div>
  </div>
  <div class="p-3 chatbox p-3 m-3">
    <h2>Chat:</h2>
    <input class="w-75 mb-3" id="chatBox" />
    <br />
  <div>
    <span class="date" id="date"></span>
    <span class="message" id="message"></span>
  </div>
</div>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>